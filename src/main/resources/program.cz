#include <MDA>

enum COLOR = [RED, YELLOW, GREEN, BLUE, PINK, PURPLE];

int main() {
    enum COLOR my_color = COLOR.YELLOW;

    switch (my_color) {
        case COLOR.YELLOW:
            print("YELLOW\n");
            break;
        default:
            print("UNKNOWN\n");
            break;
    }

    print("My color: ", my_color);

    return 0;
}